cell mkGlobalState(
  int pk1, int pk2,
  int share1, int share2,
  int timeout,
  int fineAmount,
  int nonce) {

  return begin_cell()
    .store_uint(256, pk1)
    .store_uint(256, pk2)
    .store_uint(120, share1)
    .store_uint(120, share2)
    .store_uint(32, timeout)
    .store_int(120, fineAmount)
    .store_uint(64, nonce)
    .end_cell();
}

;; cell -> (pk1, pk2, share1, share2, timeout, fineAmount, nonce)
_ parseGlobalState(cell data) {
  slice ds = data.begin_parse();
  var res =
    ( ds~load_ref()     ;; pk1
    , ds~load_ref()     ;; pk2
    , ds~load_uint(120) ;; share1
    , ds~load_uint(120) ;; share2
    , ds~load_uint(32)  ;; timeout
    , ds~load_int(120)  ;; fineAmount
    , ds~load_uint(64)  ;; nonce
    );
  ds.end_parse();
  return res;
}
